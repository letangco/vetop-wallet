{
  "name": "vetop-wallet",
  "version": "1.0.0",
  "private": true,
  "description": "Vetop wallet framework service",
  "scripts": {
    "args": "npm run $port",
    "start": "nodemon server.js",
    "develop": "cross-env NODE_ENV=development node server.js",
    "production": "cross-env NODE_ENV=production node server.js",
    "debug": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon --inspect-brk=0.0.0.0:9339 init.js",
    "dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon --experimental-modules init.js",
    "staging": "cross-env NODE_ENV=staging pm2 start server.js -i max --name vetop-wallet",
    "development": "cross-env NODE_ENV=development pm2 start server.js -i max --name vetop-wallet",
    "prod": "cross-env NODE_ENV=production pm2 start server.js -i max --name vetop-wallet",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint server"
  },
